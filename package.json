{
  "name": "touch-nav",
  "version": "3.0.2",
  "description": "",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "assets": {
    "assets_in": "test_assets",
    "assets_out": "app/assets",
    "name": "server",
    "javascripts": {
      "ext": [
        ".js",
        ".es6.js"
      ],
      "files": [
        "application.js"
      ],
      "ignore": [],
      "externals": [],
      "separate": true,
      "input": "test_assets/javascripts",
      "output": "app/assets/javascripts",
      "common": "common.js",
      "transforms": [
        [
          "babelify",
          {
            "presets": [
              "es2015",
              "react"
            ]
          }
        ]
      ]
    },
    "karma": {
      "input": "./spec"
    },
    "port": 9090,
    "rails_port": 3030,
    "uiport": 8080
  },
  "config": {
    "assets_in": "test_assets",
    "assets_out": "app/assets",
    "name": "server",
    "port": 4140,
    "rails_port": 3030,
    "uiport": 4141
  },
  "img_config": {
    "ext": [
      "*.png",
      "*.gif",
      "*.jpg",
      "*.jpeg",
      "*.svg"
    ]
  },
  "js_config": {
    "ext": [
      ".js",
      ".es6.js"
    ],
    "files": [
      "application.js"
    ],
    "input": "app/assets_uncompiled/javascripts",
    "output": "app/assets/javascripts",
    "common": "common.js"
  },
  "scripts": {
    "browserSync": "node ./fe_build_scripts/browserSync.js",
    "build": "babel src -x '.js' --source-maps  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps -d lib",
    "js": "asset-pipeline-browserify",
    "js:minify": "asset-pipeline-browserify -m",
    "js:watch": "asset-pipeline-browserify -w",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "sass:build": "node ./fe_build_scripts/sass-build.js -m",
    "sass:watch": "node ./fe_build_scripts/sass-build.js -w",
    "start": "npm run browserSync & npm run build:watch",
    "test": "asset-pipeline-karma",
    "test:watch": "asset-pipeline-karma -w",
    "test:mac": "asset-pipeline-karma -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/touch-nav.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/djforth/touch-nav/issues"
  },
  "homepage": "https://github.com/djforth/touch-nav#readme",
  "devDependencies": {
    "@djforth/ap_browserify": "^1.4.0",
    "@djforth/ap_imagemin": "2.0.7",
    "@djforth/ap_karma": "1.1.12",
    "@djforth/ap_sprites": "1.0.10",
    "@djforth/ap_utils": "^2.0.13",
    "@djforth/morse-jasmine": "^1.0.17",
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.12.5",
    "chokidar": "~1.4.3",
    "eslint": "^2.9.0",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-react": "^5.0.1",
    "event-stream": "^3.3.2",
    "factor-bundle": "^2.5.0",
    "image-size": "^0.5.0",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.7.0",
    "node-sass-globbing": "0.0.23",
    "path": "^0.12.7",
    "react-addons-test-utils": "~15.0.2",
    "react-jasmine": "~1.1.1",
    "readdirp": "^2.0.0",
    "through2": "^2.0.1",
    "classnames": "^2.2.5",
    "flux": "^2.1.1",
    "immutable": "^3.8.1",
    "lodash": "~4.12.0",
    "morse-react-mixins": "~1.2.0",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "viewport-detection-es6": "~2.1.2"
  },
  "peerDependencies": {
    "classnames": "^2.2.*",
    "flux": "^2.1.*",
    "immutable": "^3.8.*",
    "lodash": "^4.*.*",
    "morse-react-mixins": "^1.2.*",
    "react": "^15.0.*",
    "react-dom": "^15.0.*",
    "viewport-detection-es6": "^2.1.*"
  }
}
